{
  "name": "mern-app",
  "version": "1.0.0",
  "description": "Basic MERN stack application. Uses webpack.",
  "main": "template.js",
  "scripts": {
    "dev": "nodemon",
    "start": "NODE_ENV=production node ./dist/server.generated.js",
    "build": "webpack --config webpack.config.client.production.js && webpack --mode=production --config webpack.config.server.js"
  },
  "keywords": [
    "MongoDB",
    "Express",
    "React",
    "Node.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VCAngel/Webpack-Boilerplate/templates/MERN"
  },
  "author": "VCAngel",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.1",
    "mongodb": "^4.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-stage-2": "^7.8.3",
    "@babel/runtime": "^7.18.3",
    "@svgr/webpack": "^6.2.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-macros": "^3.1.0",
    "nodemon": "^2.0.16",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-node-externals": "^3.0.0"
  }
